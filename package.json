{
  "name": "cyan-rest-generator",
  "version": "0.1.1",
  "description": "rest interface generator",
  "main": "dist/extension.js",
  "bin": {
    "rest-generator": "dist/cli.js"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "generateRestInterface",
        "title": "Generate REST interface"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "generateRestInterface",
          "when": "resourceFilename =~ /.*controller.*\\.ts$/i",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "generateRestInterface",
          "when": "resourceFilename =~ /.*controller.*\\.ts$/i",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "build": "rm -rf dist/* && tsc",
    "prepare": "npm run build",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steinsroka/rest-generator.git"
  },
  "author": "steinsroka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/steinsroka/rest-generator/issues"
  },
  "homepage": "https://github.com/steinsroka/rest-generator#readme",
  "dependencies": {
    "commander": "^12.1.0",
    "typescript": ">=4.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.0.0",
    "@types/vscode": "^1.96.0",
    "ts-node": "^10.0.0"
  }
}
